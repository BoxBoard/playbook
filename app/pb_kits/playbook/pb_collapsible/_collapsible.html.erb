<%= content_tag(:div,
    aria: object.aria,
    class: object.classname,
    data: object.data,
    id: object.id) do %>
    <% if object.children %>
    <%= pb_rails("collapsible/collapsible_content", props: { padding: object.body_padding, children: object.children }) %>
  <% end %>
<% end %>

<i class="fas fa-chevron-up"></i>
