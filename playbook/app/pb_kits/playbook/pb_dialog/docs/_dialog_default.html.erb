<script>
  document.addEventListener("DOMContentLoaded", function(event) { 
    document.querySelector("#dialog-trigger").addEventListener("click", function() {
      alert("Hello World!");
    });
  });
</script>


<%= pb_rails("dialog", props: {id: 'testing'}) %>
