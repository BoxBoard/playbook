
<%= pb_rails("dialog", props: {id: 'testing', ref: 'Dialog => { window.Dialog = Dialog }'}) %>
<%= pb_rails("button", props: {id: 'dialog-trigger', text: "Open Dialog" }) %>


<script>
  document.addEventListener("DOMContentLoaded", function(event) { 
    document.querySelector("#dialog-trigger").addEventListener("click", function() {
        window.Dialog.setState({ isOpen: true })
    });
  });
</script>
